	<parameter name="SEDE" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
    detalle.fecha,
    file.pax,
    detalle.num_personas,
    file.nro_correlativo,
    service.descripcion,
    vehi.descripcion as vehi_descrip,
    detalle.precio_servicio,
    detalle.descuento,
    detalle.adicional,
	detalle.dias_viaje,
    cliente.nombre,
    cliente.razonsocial,
cliente.direccion,
cliente.numerodocumento
from
    file as file
        inner join
    servicio_detalle as detalle on file.idfile = detalle.idfile
        inner join
    cliente as cliente on cliente.idcliente = file.idfile
        inner join
    servicio as service on detalle.idservicio = service.idservicio
        inner join
    vehiculo as vehi on detalle.idvehiculo = vehi.idvehiculo
        inner join
    tipo_vehiculo as tipo_vehi on vehi.idtipo_vehiculo = tipo_vehi.idtipo_vehiculo
where
    file.idfile = $P{IDFILE}
and detalle.gravada = $P{GRAVADA}
order by detalle.fecha
]]>
	</queryString>
